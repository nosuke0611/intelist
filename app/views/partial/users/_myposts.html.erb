<% provide(:title, '投稿一覧') %>
<div>
  <ul class='posts'>
    <% if @myposts.any? %>
      <div class='post'>
        <% @myposts.each do |post| %>
          <%= render 'partial/posts/post_list', post: post %>
        <% end %>
      </div>
    <% else %>
      <ul>投稿はまだありません</ul>
    <% end %>
  </ul>
  <%= paginate @myposts %>
</div>

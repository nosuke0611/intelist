<% provide(:title, 'いいねした投稿') %>
<div>
  <ul class='posts'>
    <% if @favposts.any? %>
      <div class='post'>
        <% @favposts.each do |post| %>
          <%= render 'partial/posts/post_list', post: post %>
        <% end %>
      </div>
    <% else %>
      <ul>いいねした投稿はまだありません</ul>
    <% end %>
  </ul>
  <%= paginate @favposts %>
</div>
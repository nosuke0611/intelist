$('#follow-form-<%= @user.id %>').html('<%= j render "partial/users/follow" %>');
$(function(){
  if($('#followers').length){
    $('#followers').html('フォロワー数｜　<%= @user.followers.count %>人');
  }
});
$(function(){
  if($('#user-<%= @user.id %>-followers').length){
    $('#user-<%= @user.id %>-followers').html('<%= link_to @user.followers.count, followers_user_path(@user) %>');
  }
});